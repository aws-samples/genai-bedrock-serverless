{
	"openapi": "3.0.0",
	"info": {
		"title": "Error Description API",
		"description": "Gets internal error description",
		"version": "1.0.0"
	},

	"paths": {
		"/get_error_description": {
			"get": {
				"description": "Get error description based on HTTP error code and timestamp ",
				"operationId": "geterrordescription",
				"parameters": [
					{
						"name": "httperrorcode",
						"in": "query",
						"description": "HTTP error code of the error",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "timestamp",
						"in": "query",
						"description": "timestamp of the error",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Error description based on error entry in log file" ,
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"errorDescription": {
											"type": "string"
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
